{
  "name": "transform-css-logic",
  "displayName": "transform-css-logic",
  "description": "css properties to logical properties",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "transform-css-logic.transformFile",
        "title": "Transform to css-logic"
      }
    ],
    "keybindings": [
      {
        "command": "transform-css-logic.transformFile",
        "key": "ctrl+t",
        "mac": "cmd+t",
        "when": "editorTextFocus && resourceFilename =~ /.css$|.less$|.scss$|.sass$/"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "transform-css-logic.transformFile",
          "group": "navigation",
          "when": "resourceFilename =~ /.css$|.less$|.scss$|.sass$/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "postcss": "^8.4.21",
    "postcss-rtl-logical-properties": "^1.0.7"
  }
}
